{

      "configuration": {
        "host" : "localhost",
        "port" : 9000,
        "endpoint": "paraview",
        "content": "/home/cjh/work/build/ParaView/www",
        "proxy_file" : "/opt/websim/proxy.txt",
        "sessionURL" : "wss://${host}/local_proxy?sessionId=${id}",
        "timeout" : 5,
        "log_dir" : "/tmp",
        "upload_dir" : "/.../data",
        "fields" : ["girder", "token", "port", "mesh"]
      },

      "sessionData" : { "updir": "/Home" },

      "resources" : [ { "host" : "localhost", "port_range" : [9001, 9999] } ],

      "properties" : {
        "build_dir" : "/home/cjh/work/build/ParaView",
        "python_exec" : "/opt/websim/paraview/install/bin/pvpython",
        "source_dir": "/home/cjh/work/source/ParaView"
        "girder": "http://{{ groups['girder'][0] }}:8080/api/v1"
        "meshViewer" : "/opt/websim/cmb-web/scripts/hydra-ne/pv_mesh_viewer.py"
      },


      "apps" : {
        "mesh-viewer":
          "cmd" : [
            "${python_exec}", "${meshViewer}", "--port", "${port}", "-f", "--token", "${token}", "--url", "${girder}", "--mesh", "${mesh}"
              ],
          "ready_line" : "Starting factory"
        }
}

